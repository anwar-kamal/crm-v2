main.flex-grow.grid.gap-6.overflow-hidden.content-start.relative(class="p-3 md:py-6 md:px-6  lg:py-8 lg:px-12" x-data="{ tab: 'all' }")
	include ./head-tabs
	a(href="").header__primary__start All My Leads
	.grid(class=" grid-cols-12 md:gap-x-6 gap-x-2 gap-y-6") 
		form(class="md:col-span-5 col-span-9")
			label(for="default-search", class="mb-2 text-sm font-medium text-gray-900 sr-only dark:text-white") Search
			div(class="relative")
				div(class="absolute inset-y-0 start-0 flex items-center ps-3 pointer-events-none")
					svg(class="w-4 h-4 text-gray-500 dark:text-gray-400", aria-hidden="true", xmlns="http://www.w3.org/2000/svg", fill="none", viewBox="0 0 20 20")
						path(stroke="currentColor", stroke-linecap="round", stroke-linejoin="round", stroke-width="2", d="m19 19-4-4m0-7A7 7 0 1 1 1 8a7 7 0 0 1 14 0Z")
				input#default-search(type="search", class=" w-full !px-9", placeholder="Search by name , phone ...", required)
		div(class="col-span-3 w-full  md:hidden" @click="filter = true")
			+button-sec('',"-ml-2   fa-solid fa-filter","w-full h-full")
		div.hidden(class="lg:block")
		div( class=" relative col-span-12 md:col-span-5 lg:col-span-4 bg-white rounded-2xl border border-gray-300   grid  content-start overflow-hidden text-xs sm:text-sm lg:text-base cent" )
			ul(class=" w-full whitespace-nowrap p-1  overflow-x-auto  gap-1 grid grid-cols-3 items-stretch " )
				li(class=" cursor-pointer  hover:bg-primary hover:bg-opacity-10 hover:text-primary  tracking-wider font-semibold px-1 py-2 text-center rounded-2xl  " :class="tab === 'all'? ' bg-primary bg-opacity-10  text-primary':'text-neutral-700'" @click="tab = 'all'")
					button All
				li(class=" cursor-pointer  hover:bg-primary hover:bg-opacity-10 hover:text-primary tracking-wider font-semibold px-1 py-2 rounded-2xl text-center" :class="tab === 'own' ? ' bg-primary bg-opacity-10  text-primary':'text-neutral-700 '"  @click="tab = 'own'")
					button My Own
				li(class=" cursor-pointer  hover:bg-primary hover:bg-opacity-10 hover:text-primary text-neutral-700 tracking-wider font-semibold px-1 py-2 rounded-2xl text-center" :class="tab === 'followUp'? ' bg-primary bg-opacity-10  text-primary':'text-neutral-700'" @click="tab = 'followUp'")
					button My follow up
		div(class="md:col-span-2 w-full hidden md:block " @click="filter = true")
			+button-sec('Filter',"fa-solid fa-filter","w-full h-full")
